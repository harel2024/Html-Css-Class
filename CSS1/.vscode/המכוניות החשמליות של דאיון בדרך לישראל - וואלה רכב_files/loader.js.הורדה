    //     var imageFailedFunc = imageFailed.bind(this, elem, newImage, recommendation.thumbnail, cloudinaryTimer);

    //     newImage.addEventListener('load', imageLoadedFunc, true);
    //     newImage.addEventListener('error', imageFailedFunc, true);
    //     newImage.src = newImageUrl;
    //     newImage.alt = recommendation.title;
    //     newImage.setAttribute('data-tbl-swap', true);

    //     observer.disconnect();
    // }
    /** END CLOUDINARY HOOK CODE */

    function imageDimMap(isLargeItem) {
        switch (recommendation.placement) {
            case "HP Main Taste":
                return 300;
            default:
                return isLargeItem ? 500 : 200;

        }
    }

    function calculateImageDim() {
        var imageContainer = elem.querySelector('[data-tb-thumbnail]');
        var isLargeItem = imageContainer.clientWidth > 143;
        var imageWidht = imageDimMap(isLargeItem);
        var imageHeight = imageContainer.parentElement.clientHeight * 2;

        return {
            w: imageWidht,
            h: imageHeight
        };
    }

    function createNewImageUrl(url) {
        var urlParts = new URL(url);
        var dimension = calculateImageDim();
        var dimParams = '/f_auto,q_auto,' + 'w_' + dimension.w + ',t_54';
        return urlParts.origin + dimParams + urlParts.pathname;
    }

    function fixImageRatio(img, imgWrapper) {
        var imgPlaceholder = imgWrapper.querySelector('.lazyload-placeholder');
        img.style.width = (imgPlaceholder.clientWidth + 20) + 'px';
        img.style.objectFit = 'cover';
    }

    function handleIcons() {
        var iconHtml = '<div class=\"play-indicator css-1g64e3l mobile-small  walla-icon-play-with-border\"></div>';
        var iconElem = elem.querySelector('.play-indicator');
        if (recommendation.type === 'video') {
            if (iconElem) {
                return;
            } else {
                var elemToInsertInto = elem.querySelector('.lazyload-wrapper');
                if (elemToInsertInto) {
                    elemToInsertInto.insertAdjacentHTML('afterbegin', iconHtml);
                }
            }
        } else if (iconElem) {
            iconElem.setAttribute('style', 'display: none !important;');
        }
    }

    function callback(mutationList, observer) {
        handleIcons();
        var origImgElem = elem.querySelector('img:not([data-tbl-swap])');
        if (origImgElem) {
            origImgElem.style.display = 'none';
        }
    }


    function handleKicker() {
        if (recommendation.placement !== 'HP Main Taste')
            return;
        var kickerElem = elem.querySelector('[data-tb-kicker]');
        if (kickerElem) {
            kickerElem.setAttribute('style', 'display: none !important;');
        }
    }

    function createNewImageElem() {
        var newImageUrl = createNewImageUrl(recommendation.thumbnail);
        var newImage = document.createElement('img');
        newImage.src = newImageUrl;
        newImage.alt = recommendation.title;
        newImage.setAttribute('data-tbl-swap', true);
        return newImage;
    }

    function handleImage() {
        var imgElemExists = elem.querySelector('[data-tb-thumbnail] img');
        if (imgElemExists) return;
        var newImageElem = createNewImageElem();
        var imageWrapper = elem.querySelector('[data-tb-thumbnail]');
        imageWrapper.insertAdjacentElement('afterbegin', newImageElem);
        fixImageRatio(newImageElem, imageWrapper);
    }

    function handleImageMutation() {
        handleImage();
        var observerElem = elem.querySelector('[data-tb-thumbnail]');

        var observer = new MutationObserver(callback);
        observer.observe(observerElem, {
            childList: true,
            subtree: true
        });
    }

    handleKicker();
    handleImageMutation();
