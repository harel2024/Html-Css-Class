    function imageDimMap(isLargeItem) {
        switch (recommendation.placement) {
            case "HP Main Taste":
                return 300;
            default:
                return isLargeItem ? 500 : 200;

        }
    }

    function calculateImageDim() {
        var isLargeItem = imgElem.clientWidth > 143;
        var imageWidht = imageDimMap(isLargeItem);

        return {
            w: imageWidht,
        };
    }

    function createNewImageUrl(url) {
        var urlParts = new URL(url);
        var dimension = calculateImageDim();
        var dimParams = '/f_auto,q_auto,' + 'w_' + dimension.w + ',t_54';
        return urlParts.origin + dimParams + urlParts.pathname;
    }

    return createNewImageUrl(originalUrl);
},"hp4u-script-config-pc-wallail-walla":function(elem, recommendation) {
    /*** This code is for using Cloudinary. Swap for Walla works more smoothly when using Walla's cdn */
    /** START CLOUDINARY HOOK CODE */
    // function createNewImageUrl(originalUrl) {
