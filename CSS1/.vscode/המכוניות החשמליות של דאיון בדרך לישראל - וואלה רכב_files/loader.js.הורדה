/** [DEV-167809] - Start - Manipulate Main Taste Region Urls For HP4U **/
	

        function manipulateUrl(url, paramValue) {
            var hasQueryString = url.indexOf('?') !== -1;
            var separator = hasQueryString ? '&' : '?';
            return url + separator + paramValue;
        }

        TRC.manipualteRegion = function manipulateTastingsRegion() {
            var tastingItems = document.querySelectorAll('[data-tb-region="Main Taste"] li:not(.marketing)');
						if (!tastingItems) {
								console.log('No data-tb-region Found');
								return;
								}
					
            var validElems = Array.prototype.filter.call(tastingItems, function(li) {
                return li.querySelector('a') !== null;
            });
            if (validElems.length === 0) {
                return;
            }
					
            validElems.forEach(function(card, index) {
							var anchorElem = card.querySelector('[data-tb-link]');
							if (!anchorElem) {
								console.log('No data-tb-link Found');
							}
            
                if (!anchorElem) {
                    return;
                }
							
                var url = anchorElem.href;
                var urlSuffix = index < 3 ? 'tb_ref=1-3-w' : 'tb_ref=4-8-w';
                var manipulatedUrl = manipulateUrl(url, urlSuffix);
                anchorElem.href = manipulatedUrl;
            });
        }
	
	     TRC.listen('trcResponseHandled', function(response) {
              if (TRC.tastingsUrlManipulatedOnce) {
                    return;
                }
              
                TRC.tastingsUrlManipulatedOnce = TRC.tastingsUrlManipulatedOnce || 0;
                if (!(response && response.res && response.res.trc && response.res.trc.evi)) {
										console.log('No evi in res');
                    return;
                }
				 
         
                if (response.res.trc.evi[2] && response.res.trc.evi[2].indexOf('18281') > -1) {
                    TRC.tastingsUrlManipulatedOnce = 1;
                    TRC.manipualteRegion();
										console.log('Teimot Manipulated url');
                }
              
            });
	          
/** [DEV-167809] - End - Manipulate Main Taste Region Urls For HP4U **/

